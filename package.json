{
  "name": "vf-project-template",
  "version": "2.0.1",
  "scripts": {
    "node": "node",
    "ts": "npm run node -- -r ts-node/register",
    "cdk": "cdk",
    "postinstall": "check-engine",
    "bootstrap": "npm run ts bin/bootstrap",
    "codebuild": "npm run ts bin/runCodeBuild",
    "synth": "npm run synth -ws",
    "deploy": "npm run deploy -ws",
    "destroy": "npm run destroy -ws",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "use-ca": "npm run ts bin/useCodeArtifact",
    "use-github": "npm run ts bin/useGithub",
    "pub": "npm run ts bin/publishToCodeArtifact",
    "installPackFiles": "npm run node -- -r ts-node/register 'bin/installPackFiles'"
  },
  "devDependencies": {
    "@aws-sdk/client-connect": "^3.38.0",
    "@aws-sdk/client-dynamodb": "^3.31.0",
    "@aws-sdk/client-lambda": "^3.34.0",
    "@aws-sdk/lib-dynamodb": "^3.39.0",
    "@aws-sdk/signature-v4-crt": "^3.39.0",
    "@aws-sdk/util-dynamodb": "^3.39.0",
    "@shelf/jest-dynamodb": "^2.1.0",
    "@ttec-dig-vf/cdk-resources": "^7.2.1",
    "@ttec-dig-vf/vf-logger": "^0.1.6",
    "@types/aws-lambda": "^8.10.84",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "aws-cdk": "^2.19.0",
    "aws-cdk-lib": "^2.19.0",
    "aws-sdk": "^2.1008.0",
    "check-engine": "^1.10.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.28",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.16",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.2"
  },
  "engineStrict": true,
  "engines": {
    "node": "14",
    "npm": ">=7.0.0",
    "git": ">=2.2.3"
  },
  "workspaces": [
    "packages/cicd",
    "packages/connect-core",
    "packages/connect-lambdas",
    "packages/admin",
    "packages/voicemail"
